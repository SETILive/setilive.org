<div id='subjectDetails'>
	<h1> Subject id :  <%=@zooniverse_id %></h1>
	<h2> Freq : <%=@location.freq%> </h2>
	<h2> time : <%=@location.freq%> </h2>
	
	<p> <a href='http://talk.setilive.org/observation_groups/<%-@zooniverse_id%>'> View on talk </a></p>
	<% for observation in @observations : %>
		<div class='observationContainer'>

			<img src='<%=observation.image_url%>' />	
		</div>
	<%end%>
</div>