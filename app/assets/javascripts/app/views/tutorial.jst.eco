<div id="inline_tutorial_box"> 
  <div class="content">
    <h1 class="tutorial_header"><%= @title %></h1> 
    <p class="tutorial_text"><%= @text %></p>
  </div>
  <div class="controls">
    <% if @navigate: %>
      <span class="tutorial_skip">Skip to Part 2</span>
      <span class="tutorial_next">next</span>
    <% else: %>
      <span class="tutorial_next">next</span>
    <% end %>
  </div>
</div>  